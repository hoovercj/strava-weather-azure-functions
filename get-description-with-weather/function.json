{
  "disabled": false,
  "bindings": [
    {
      "route": "description/{activityid}",
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "tableName": "StravaWeatherman",
      "name": "activityWeather",
      "type": "table",
      "partitionKey": "activityWeather",
      "rowKey": "{activityid}",
      "direction": "in"
    },
    {
      "tableName": "StravaWeatherman",
      "name": "processedActivity",
      "type": "table",
      "partitionKey": "processedActivities",
      "rowKey": "{activityid}",
      "direction": "in"
    },
    {
      "tableName": "StravaWeatherman",
      "name": "outTableBinding",
      "type": "table",
      "direction": "out"
    },
    {
      "name": "currentApiLimits",
      "type": "blob",
      "path": "api/limits",
      "direction": "in"
    },
    {
      "name": "newApiLimits",
      "type": "blob",
      "path": "api/limits",
      "direction": "out"
    }
  ]
}